{
    "name": "osiemsiedem/laravel-pluralization-fix",
    "description": "A Laravel package for fixing pluralization issues.",
    "keywords": ["laravel", "pluralization", "i18n"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marek Szymczuk",
            "email": "marek@osiemsiedem.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.7.*|5.8.*",
        "illuminate/translation": "5.7.*|5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4" : {
            "OsiemSiedem\\Translation\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OsiemSiedem\\Tests\\Translation\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OsiemSiedem\\Translation\\TranslationServiceProvider"
            ]
        }
    }
}
